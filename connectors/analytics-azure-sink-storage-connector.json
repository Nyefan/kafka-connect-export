{
  "connector.class": "io.confluent.connect.azure.blob.AzureBlobStorageSinkConnector",
  "topics": "analytics_events",
  "locale": "en-US",
  "key.converter": "org.apache.kafka.connect.storage.StringConverter",
  "value.converter": "org.apache.kafka.connect.json.JsonConverter",
  "value.converter.schemas.enable": "false",
  "confluent.topic.bootstrap.servers": "analytics-common-kafka.dev.svc.cluster.local:9092",
  "confluent.topic.replication.factor": 1,
  "rotate.interval.ms": 600000,
  "flush.size": 10000,
  "azblob.account.name": "devradiusstorage",
  "azblob.account.key": "",
  "azblob.container.name": "kafka-connect-export",
  "format.class": "io.confluent.connect.azure.blob.format.json.JsonFormat",
  "az.compression.type": "none",
  "partitioner.class": "io.confluent.connect.storage.partitioner.TimeBasedPartitioner",
  "partition.duration.ms": 3600000,
  "timestamp.extractor": "Record",
  "path.format": "'analytics'/YYYY/MM/dd",
  "timezone": "UTC"
}